<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="udemy.course.summary.android.app">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SummaryApplication"
        tools:targetApi="31">
        <activity
            android:name=".recyclerview.linearlayout.RecyclerviewLinearLayoutActivity"
            android:exported="false" />
        <activity
            android:name=".navigation.training.HomeNavActivity"
            android:exported="false" />
        <activity
            android:name=".bindings.twoway.MainWayTwoBindActivity"
            android:exported="false" />
        <activity
            android:name=".bindings.with.livedata.BindingWithLiveDataActivity"
            android:exported="false" />
        <activity
            android:name=".bindings.MainFirstBindActivity"
            android:exported="false" />
        <activity
            android:name=".navigation.challenge.OrderNavChallengeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>